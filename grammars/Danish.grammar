/* Imperative form - base verb stem */
<verb-imperative> ::= <verb-stem> | <irregular-imperative>

<irregular-imperative> ::= "vær" | "hav" | "bliv" | "lad"

/* Object (direct and indirect) with clause complements */
<object> ::= <direct-object>

<direct-object> ::= <noun-phrase>
                 | <pronoun-object>
                 | <clause-object>
                 | <infinitive-phrase>

/* Clause as object (complement clause) */
