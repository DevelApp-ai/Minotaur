Grammar: Danish
TokenSplitter: Space
FormatType: CEBNF
ContextSensitive: true
SymbolTableSharing: hierarchical

/*
 * Danish Natural Language Grammar - Comprehensive Edition
 * 
 * This grammar provides extensive coverage of Danish language structures including:
 * - Regular and irregular verb conjugations (all tenses and moods)
 * - Passive voice and reflexive verbs
 * - Context-sensitive agreement rules (gender, number, case, definiteness)
 * - Full regular expression support for morphological patterns
 * - Common and neuter gender agreement
 * - Definite and indefinite forms
 * - Multiple tenses (present, past, future, perfect, pluperfect)
 * - Conditional and subjunctive moods
 * - V2 word order and topicalization
 * - Relative clauses and subordinate structures
 * - Comparative and superlative adjectives
 * - Genitive case and compound nouns
 * - Negation patterns
 * - Quantifiers and determiners
 * - Reflexive and reciprocal pronouns
 * - Complex temporal expressions
 */

Keywords: og, eller, men, hvis, når, fordi, at, som, der, det, den, de, jeg, du, han, hun, vi, I, de, mig, dig, ham, hende, os, jer, dem, min, din, hans, hendes, vores, jeres, deres, denne, dette, disse, en, et, ikke, kan, vil, skal, må, have, være, blive, sig, hinanden, selv, nej, ja, måske, altså, dog, endda, også, heller, jo, vel, nok, bare, lige, godt, nå, så

/* Top-level sentence structure with V2 word order */
<sentence> ::= <main-clause>
            | <subordinate-clause>
            | <compound-sentence>
            | <complex-sentence>
            | <multi-embedded-sentence>
            | <coordinated-complex-sentence>

/* Main clause with V2 word order (verb-second) */
<main-clause> ::= <declarative-clause>
               | <interrogative-clause>
               | <imperative-clause>
               | <exclamative-clause>
               | <parenthetical-clause>

/* V2 declarative: TopicField + FiniteVerb + MiddleField + MainVerb + FinalField */
<declarative-clause> ::= <topic-field> <opt-whitespace> <finite-verb> <opt-whitespace> <middle-field>? <opt-whitespace> <verb-complement>? <opt-whitespace> <final-field>? <punctuation>?

/* Topic field can contain various constituents */
<topic-field> ::= <subject>
               | <adverbial>
               | <object>
               | <prepositional-phrase>

/* Middle field contains subject (if not topicalized), objects, and adverbials */
<middle-field> ::= <middle-field-element> ( <opt-whitespace> <middle-field-element> )*

<middle-field-element> ::= <subject>
                        | <negation>
                        | <sentence-adverb>
                        | <object>
                        | <indirect-object>
                        | <reflexive-pronoun>

/* Negation */
<negation> ::= "ikke" | "aldrig" | "ingenting" | "ingen" | "intet"

/* Sentence adverbs */
<sentence-adverb> ::= "altid" | "ofte" | "sjældent" | "måske" | "nok" | "jo" | "vel" | "da" | "dog" | "også" | "heller" | "endda" | "bare" | "lige" | "godt" | "vist" | "nødvendigvis"

/* Verb complement (infinitive, participle, predicate adjective) */
<verb-complement> ::= <infinitive-phrase>
                   | <past-participle>
                   | <present-participle>
                   | <predicate-adjective>

<infinitive-phrase> ::= ( "at" <opt-whitespace> )? <verb-infinitive> ( <opt-whitespace> <object> )?

<predicate-adjective> ::= <adjective>

/* Final field for heavy constituents */
<final-field> ::= <subordinate-clause>
               | <relative-clause>
               | <prepositional-phrase>
               | <embedded-clause-sequence>
               | <extraposed-clause>

/* Embedded clause sequence (multiple subordinate clauses) */
<embedded-clause-sequence> ::= <subordinate-clause> ( <opt-whitespace> <coordinating-conjunction>? <opt-whitespace> <subordinate-clause> )+

/* Extraposed clause (postponed subordinate clause with correlate) */
<extraposed-clause> ::= <correlate-pronoun> <opt-whitespace> <subordinate-clause>

<correlate-pronoun> ::= "det"

/* Interrogative clause */
<interrogative-clause> ::= <yes-no-question>
                        | <wh-question>

<yes-no-question> ::= <finite-verb> <opt-whitespace> <subject> <opt-whitespace> <middle-field>? <opt-whitespace> <verb-complement>? <opt-whitespace> <final-field>? "?"

<wh-question> ::= <question-word> <opt-whitespace> <finite-verb> <opt-whitespace> <subject>? <opt-whitespace> <middle-field>? <opt-whitespace> <verb-complement>? <opt-whitespace> <final-field>? "?"

/* Imperative clause */
<imperative-clause> ::= <verb-imperative> <opt-whitespace> <middle-field>? <opt-whitespace> <object>? <opt-whitespace> <adverbial>? ( "!" | "." )?

/* Exclamative clause */
<exclamative-clause> ::= <exclamative-word> <opt-whitespace> <adjective>? <opt-whitespace> <noun-phrase>? <opt-whitespace> <finite-verb>? <opt-whitespace> <middle-field>? "!"

<exclamative-word> ::= "hvor" | "sikke" | "så" | "hvilket" | "hvilken"

/* Subordinate clause (verb-final word order) with nesting support */
<subordinate-clause> ::= <simple-subordinate-clause>
                      | <nested-subordinate-clause>
                      | <coordinated-subordinate-clause>

<simple-subordinate-clause> ::= <subordinating-conjunction> <opt-whitespace> <subordinate-content>

/* Nested subordinate clause (sub-sentence within sub-sentence) */
<nested-subordinate-clause> ::= <subordinating-conjunction> <opt-whitespace> <nested-subordinate-content>

<nested-subordinate-content> ::= <subject> <opt-whitespace> <middle-field>? <opt-whitespace> <verb-phrase-subordinate> <opt-whitespace> <nested-final-field>

<nested-final-field> ::= <subordinate-clause>
                      | <relative-clause>
                      | <embedded-clause-sequence>
                      | <prepositional-phrase>

/* Coordinated subordinate clauses */
<coordinated-subordinate-clause> ::= <subordinate-clause> <opt-whitespace> <coordinating-conjunction> <opt-whitespace> <subordinate-clause>

<subordinate-content> ::= <subject> <opt-whitespace> <middle-field>? <opt-whitespace> <verb-phrase-subordinate> <opt-whitespace> <subordinate-final-field>?

<subordinate-final-field> ::= <subordinate-clause>
                           | <relative-clause>
                           | <prepositional-phrase>
                           | <adverb-clause>

<verb-phrase-subordinate> ::= ( <auxiliary-verb> <opt-whitespace> )* <main-verb>

/* Relative clause with nesting support */
<relative-clause> ::= <simple-relative-clause>
                   | <nested-relative-clause>

<simple-relative-clause> ::= <relative-pronoun> <opt-whitespace> <subject>? <opt-whitespace> <middle-field>? <opt-whitespace> <finite-verb> <opt-whitespace> <verb-complement>?

/* Nested relative clause (relative clause containing subordinate clause) */
<nested-relative-clause> ::= <relative-pronoun> <opt-whitespace> <subject>? <opt-whitespace> <middle-field>? <opt-whitespace> <finite-verb> <opt-whitespace> <verb-complement>? <opt-whitespace> <subordinate-clause>

/* Compound sentence with multiple coordination */
<compound-sentence> ::= <simple-compound-sentence>
                     | <multi-compound-sentence>

<simple-compound-sentence> ::= <main-clause> <opt-whitespace> <coordinating-conjunction> <opt-whitespace> <main-clause>

/* Multiple coordinated main clauses (3 or more) */
<multi-compound-sentence> ::= <main-clause> ( <opt-whitespace> <coordinating-conjunction> <opt-whitespace> <main-clause> ){2,}

<coordinating-conjunction> ::= "og" | "eller" | "men" | "for" | "så" | "samt" | "eller også" | "men også" | "både" <opt-whitespace> "og" | "hverken" <opt-whitespace> "eller"

/* Complex sentence with various embedding patterns */
<complex-sentence> ::= <simple-complex-sentence>
                    | <fronted-subordinate-sentence>
                    | <split-complex-sentence>

<simple-complex-sentence> ::= <main-clause> <opt-whitespace> <subordinate-clause>

<fronted-subordinate-sentence> ::= <subordinate-clause> <opt-whitespace> "," <opt-whitespace> <main-clause>

/* Split complex sentence (subordinate clause split by main clause) */
<split-complex-sentence> ::= <subordinate-clause> <opt-whitespace> "," <opt-whitespace> <main-clause> <opt-whitespace> "," <opt-whitespace> <subordinate-clause>

/* Multi-embedded sentence (multiple levels of embedding) */
<multi-embedded-sentence> ::= <main-clause> <opt-whitespace> <subordinate-clause> ( <opt-whitespace> <subordinate-clause> )+
                           | <subordinate-clause> <opt-whitespace> "," <opt-whitespace> <main-clause> <opt-whitespace> <subordinate-clause>

/* Coordinated complex sentence (coordination at different levels) */
<coordinated-complex-sentence> ::= <complex-sentence> <opt-whitespace> <coordinating-conjunction> <opt-whitespace> <complex-sentence>
                                | <main-clause> <opt-whitespace> <coordinated-subordinate-clause>

/* Parenthetical clause (inserted remark within main clause) */
<parenthetical-clause> ::= <topic-field> <opt-whitespace> <finite-verb> <opt-whitespace> "," <opt-whitespace> <parenthetical-insertion> <opt-whitespace> "," <opt-whitespace> <middle-field>? <opt-whitespace> <verb-complement>? <punctuation>?

<parenthetical-insertion> ::= <subordinate-clause>
                           | <main-clause>
                           | <comment-phrase>

<comment-phrase> ::= <sentence-adverb> ( <opt-whitespace> <sentence-adverb> )*

/* Subject with case marking */
<subject> ::= <pronoun-subject>
           | <noun-phrase>
           | <proper-noun>
           | <infinitive-as-subject>
           | <clause-as-subject>

<infinitive-as-subject> ::= "at" <opt-whitespace> <verb-infinitive> ( <opt-whitespace> <object> )?

<clause-as-subject> ::= "at" <opt-whitespace> <subordinate-content>

/* Pronouns with case distinctions */
<pronoun-subject> ::= <personal-pronoun-nominative>
                   | <demonstrative-pronoun>
                   | <indefinite-pronoun>
                   | <interrogative-pronoun>

<personal-pronoun-nominative> ::= @CASE[nominative] ( "jeg" | "du" | "han" | "hun" | "den" | "det" | "vi" | "I" | "de" | "man" )

<personal-pronoun-accusative> ::= @CASE[accusative] ( "mig" | "dig" | "ham" | "hende" | "den" | "det" | "os" | "jer" | "dem" )

/* Reflexive pronouns */
<reflexive-pronoun> ::= @REFLEXIVE "mig" | "dig" | "sig" | "os" | "jer" | "sig" | "sig selv" | "mig selv" | "dig selv" | "os selv" | "jer selv"

/* Reciprocal pronouns */
<reciprocal-pronoun> ::= "hinanden" | "hverandre"

<pronoun> ::= <personal-pronoun-nominative>
           | <personal-pronoun-accusative>
           | <possessive-pronoun>
           | <demonstrative-pronoun>
           | <relative-pronoun>
           | <reflexive-pronoun>
           | <reciprocal-pronoun>
           | <indefinite-pronoun>
           | <interrogative-pronoun>

<possessive-pronoun> ::= <possessive-first-person>
                      | <possessive-second-person>
                      | <possessive-third-person>

/* Possessive pronouns with gender agreement */
<possessive-first-person> ::= @GENDER[common] @NUMBER[singular] "min"
                           | @GENDER[neuter] @NUMBER[singular] "mit"
                           | @NUMBER[plural] "mine"
                           | @NUMBER[plural] "vores"

<possessive-second-person> ::= @GENDER[common] @NUMBER[singular] "din"
                            | @GENDER[neuter] @NUMBER[singular] "dit"
                            | @NUMBER[plural] "dine"
                            | @NUMBER[plural] "jeres"

<possessive-third-person> ::= @GENDER[common] "hans" | "hendes" | "dens" | "deres"
                           | @GENDER[neuter] "hans" | "hendes" | "dets" | "deres"

<demonstrative-pronoun> ::= @GENDER[common] @NUMBER[singular] "denne" | "den"
                         | @GENDER[neuter] @NUMBER[singular] "dette" | "det"
                         | @NUMBER[plural] "disse" | "de"

<relative-pronoun> ::= "som" | "der" | "hvis" | "hvem" | "hvad" | "hvor" | "hvordan" | "hvorfor" | "hvornår"

/* Indefinite pronouns */
<indefinite-pronoun> ::= "man" | "nogen" | "noget" | "nogle" | "ingen" | "intet" | "en" | "et" | "enhver" | "alt" | "alle" | "hver" | "begge" | "samme" | "anden" | "andet" | "andre"

/* Interrogative pronouns */
<interrogative-pronoun> ::= "hvem" | "hvad" | "hvis" | "hvilken" | "hvilket" | "hvilke"

/* Noun Phrase with gender agreement and quantifiers */
<noun-phrase> ::= <determiner>? <opt-whitespace> <quantifier>? <opt-whitespace> <adjective-phrase>* <opt-whitespace> <noun> <opt-whitespace> <genitive-modifier>? <opt-whitespace> <relative-clause>?

<determiner> ::= <article>
              | <demonstrative-determiner>
              | <possessive-determiner>
              | <quantifying-determiner>

<article> ::= <indefinite-article> | <definite-article-standalone>

/* Indefinite articles with gender marking */
<indefinite-article> ::= @GENDER[common] "en"
                      | @GENDER[neuter] "et"

/* Standalone definite article (pre-posed) */
<definite-article-standalone> ::= @GENDER[common] @NUMBER[singular] "den"
                               | @GENDER[neuter] @NUMBER[singular] "det"
                               | @NUMBER[plural] "de"

<demonstrative-determiner> ::= @GENDER[common] @NUMBER[singular] ( "denne" | "den" | "hin" )
                            | @GENDER[neuter] @NUMBER[singular] ( "dette" | "det" | "hint" )
                            | @NUMBER[plural] ( "disse" | "de" | "hine" )

<possessive-determiner> ::= <possessive-pronoun>

/* Quantifiers */
<quantifier> ::= <numeral>
              | <indefinite-quantifier>

<numeral> ::= <cardinal-number> | <ordinal-number>

<cardinal-number> ::= "en" | "to" | "tre" | "fire" | "fem" | "seks" | "syv" | "otte" | "ni" | "ti" | "elleve" | "tolv" | "tretten" | "fjorten" | "femten" | "seksten" | "sytten" | "atten" | "nitten" | "tyve" | "tredive" | "fyrre" | "halvtreds" | "tres" | "halvfjerds" | "firs" | "halvfems" | "hundrede" | "tusind" | "million" | /[0-9]+/

<ordinal-number> ::= "første" | "anden" | "tredje" | "fjerde" | "femte" | "sjette" | "syvende" | "ottende" | "niende" | "tiende" | /[0-9]+\./ "."

<indefinite-quantifier> ::= "mange" | "få" | "flere" | "færre" | "meget" | "lidt" | "nok" | "alle" | "hver" | "enhver" | "ingen" | "begge" | "halvdelen" | "størstedelen" | "flertallet"

<quantifying-determiner> ::= "alle" | "hver" | "enhver" | "ingen" | "begge" | "samme" | "sådan"

/* Nouns with definite/indefinite, plural forms, and genitive */
<noun> ::= <simple-noun>
        | <compound-noun>
        | <noun-with-genitive>

<simple-noun> ::= <noun-common-indef>
               | <noun-neuter-indef>
               | <noun-common-def>
               | <noun-neuter-def>
               | <noun-plural-indef>
               | <noun-plural-def>

/* Regular common gender nouns (en-words) */
<noun-common-indef> ::= @GENDER[common] @NUMBER[singular] @DEFINITENESS[indefinite] <common-noun-stem>

<noun-common-def> ::= @GENDER[common] @NUMBER[singular] @DEFINITENESS[definite] <common-noun-stem> "en"

/* Regular neuter gender nouns (et-words) */
<noun-neuter-indef> ::= @GENDER[neuter] @NUMBER[singular] @DEFINITENESS[indefinite] <neuter-noun-stem>

<noun-neuter-def> ::= @GENDER[neuter] @NUMBER[singular] @DEFINITENESS[definite] <neuter-noun-stem> "et"

/* Plural forms */
<noun-plural-indef> ::= @NUMBER[plural] @DEFINITENESS[indefinite] ( <common-noun-stem> | <neuter-noun-stem> ) <plural-suffix>

<noun-plural-def> ::= @NUMBER[plural] @DEFINITENESS[definite] ( <common-noun-stem> | <neuter-noun-stem> ) <plural-suffix> "ne"?

<plural-suffix> ::= "er" | "e" | "r" | ""

/* Compound nouns (very common in Danish) */
<compound-noun> ::= <noun-stem> <linking-element>? <noun>

<linking-element> ::= "s" | "e" | "n"

/* Genitive case (possession) */
<noun-with-genitive> ::= <simple-noun> "s"

<genitive-modifier> ::= <noun-phrase> "s"

/* Common noun stems - using regex for pattern matching */
<common-noun-stem> ::= /[a-zæøå]+/

<neuter-noun-stem> ::= /[a-zæøå]+/

<noun-stem> ::= /[a-zæøå]+/

<proper-noun> ::= /[A-ZÆØÅ][a-zæøå]*/
                | <multi-word-proper-noun>

<multi-word-proper-noun> ::= /[A-ZÆØÅ][a-zæøå]*/ ( <opt-whitespace> /[A-ZÆØÅ][a-zæøå]*/ )+

/* Adjective phrases with agreement and comparison */
<adjective-phrase> ::= <degree-modifier>? <opt-whitespace> <adjective>

<degree-modifier> ::= "meget" | "rigtig" | "vældig" | "ekstremt" | "ret" | "temmelig" | "ganske" | "lidt" | "for" | "så"

<adjective> ::= <adjective-positive>
             | <adjective-comparative>
             | <adjective-superlative>

/* Positive degree with agreement */
<adjective-positive> ::= <adjective-common>
                      | <adjective-neuter>
                      | <adjective-plural>
                      | <adjective-definite>

<adjective-common> ::= @AGREEMENT[common] @DEGREE[positive] <adjective-stem>

<adjective-neuter> ::= @AGREEMENT[neuter] @DEGREE[positive] <adjective-stem> "t"?

<adjective-plural> ::= @AGREEMENT[plural] @DEGREE[positive] <adjective-stem> "e"?

<adjective-definite> ::= @AGREEMENT[definite] @DEGREE[positive] <adjective-stem> "e"

/* Comparative degree */
<adjective-comparative> ::= @DEGREE[comparative] ( <regular-comparative> | <irregular-comparative> )

<regular-comparative> ::= <adjective-stem> "ere"

<irregular-comparative> ::= "bedre" | "værre" | "større" | "mindre" | "ældre" | "yngre" | "mere" <opt-whitespace> <adjective-stem>

/* Superlative degree */
<adjective-superlative> ::= @DEGREE[superlative] ( <regular-superlative> | <irregular-superlative> )

<regular-superlative> ::= <adjective-stem> "est" | <adjective-stem> "st"

<irregular-superlative> ::= "bedst" | "værst" | "størst" | "mindst" | "ældst" | "yngst" | "mest" <opt-whitespace> <adjective-stem>

<adjective-stem> ::= /[a-zæøå]+/

/* Predicate with voice and mood */
<predicate> ::= <verb-phrase>

<verb-phrase> ::= <active-verb-phrase>
               | <passive-verb-phrase>
               | <modal-verb-phrase>

/* Active voice verb phrases */
<active-verb-phrase> ::= <simple-verb-phrase>
                      | <compound-verb-phrase>

<simple-verb-phrase> ::= <finite-verb> ( <opt-whitespace> <adverb> )?

<finite-verb> ::= <verb-present>
               | <verb-past>

<compound-verb-phrase> ::= <auxiliary-verb> <opt-whitespace> <adverb>? <opt-whitespace> <verb-complement>

/* Passive voice (s-passive and blive-passive) */
<passive-verb-phrase> ::= <s-passive>
                       | <blive-passive>

<s-passive> ::= @VOICE[passive] <verb-stem> "s"

<blive-passive> ::= @VOICE[passive] <blive-verb> <opt-whitespace> <past-participle>

/* Modal verb phrases */
<modal-verb-phrase> ::= <modal-verb> <opt-whitespace> <adverb>? <opt-whitespace> <infinitive-phrase>

/* Auxiliary verbs */
<auxiliary-verb> ::= <modal-verb>
                  | <temporal-auxiliary>
                  | <aspectual-auxiliary>

<temporal-auxiliary> ::= <have-verb> | <be-verb>

<aspectual-auxiliary> ::= <become-verb> | <continue-verb> | <begin-verb>

<modal-verb> ::= <modal-present> | <modal-past>

<modal-present> ::= @MOOD[modal] ( "kan" | "vil" | "skal" | "må" | "bør" | "tør" )

<modal-past> ::= @MOOD[modal] @TENSE[past] ( "kunne" | "ville" | "skulle" | "måtte" | "burde" | "turde" )

<continue-verb> ::= "fortsætter" <opt-whitespace> "med" <opt-whitespace> "at"
                 | "bliver" <opt-whitespace> "ved" <opt-whitespace> "med" <opt-whitespace> "at"

<begin-verb> ::= "begynder" <opt-whitespace> "at"
              | "starter" <opt-whitespace> "med" <opt-whitespace> "at"

/* Main verb with tense and mood marking */
<main-verb> ::= <verb-present>
             | <verb-past>
             | <verb-perfect>
             | <verb-pluperfect>
             | <verb-future>
             | <verb-conditional>
             | <verb-infinitive>

/* Regular verb conjugations using regex patterns */
<verb-infinitive> ::= ( "at" <opt-whitespace> )? <verb-stem>

/* Present tense - regular verbs add -r or -er */
<verb-present> ::= @TENSE[present] ( <regular-verb-present> | <irregular-verb-present> )

<regular-verb-present> ::= <verb-stem> ( "er" | "r" )

/* Common regular verb stems */
<verb-stem> ::= /[a-zæøå]+/

/* Irregular verbs in present tense - context sensitive */
<irregular-verb-present> ::= @IRREGULAR "er" | "har" | "bliver" | "går" | "kommer" | "ved" | "gør" | "får" | "ser" | "giver" | "siger" | "tager" | "finder" | "står" | "sidder" | "ligger" | "slår" | "holder" | "falder" | "dør" | "fødes" | "bærer" | "spiser" | "drikker" | "sover" | "løber"

/* Past tense - regular verbs add -(e)de or -(e)te */
<verb-past> ::= @TENSE[past] ( <regular-verb-past> | <irregular-verb-past> )

/* Regular past tense patterns */
<regular-verb-past> ::= <verb-stem> <regular-past-suffix>

<regular-past-suffix> ::= "ede" | "de" | "te"

/* Irregular verbs in past tense - context sensitive with stem changes */
<irregular-verb-past> ::= @IRREGULAR ( <irregular-past-strong> | <irregular-past-weak> )

/* Strong irregular verbs (vowel change) */
<irregular-past-strong> ::= "var" | "havde" | "blev" | "gik" | "kom" | "vidste" | "gjorde" | "fik" | "så" | "gav" | "sagde" | "tog" | "fandt" | "stod" | "sad" | "lå" | "slog" | "holdt" | "faldt" | "døde" | "fødtes" | "bar" | "spiste" | "drak" | "sov" | "løb"

/* Weak irregular verbs */
<irregular-past-weak> ::= "kunne" | "ville" | "skulle" | "måtte" | "burde" | "turde"

/* Perfect tense - har/er + past participle */
<verb-perfect> ::= @TENSE[perfect] <perfect-auxiliary> <opt-whitespace> <past-participle>

<perfect-auxiliary> ::= "har" | "er"

/* Pluperfect tense - havde/var + past participle */
<verb-pluperfect> ::= @TENSE[pluperfect] <pluperfect-auxiliary> <opt-whitespace> <past-participle>

<pluperfect-auxiliary> ::= "havde" | "var"

/* Future tense - vil/skal + infinitive */
<verb-future> ::= @TENSE[future] <future-auxiliary> <opt-whitespace> <verb-infinitive>

<future-auxiliary> ::= "vil" | "skal" | "kommer" <opt-whitespace> "til" <opt-whitespace> "at"

/* Conditional mood - ville + infinitive */
<verb-conditional> ::= @MOOD[conditional] "ville" <opt-whitespace> <verb-infinitive>

<past-participle> ::= <regular-past-participle> | <irregular-past-participle>

/* Regular past participles */
<regular-past-participle> ::= <verb-stem> ( "et" | "t" )

/* Present participle (used attributively or in continuous aspect) */
<present-participle> ::= <verb-stem> "ende"

/* Irregular past participles - context sensitive */
<irregular-past-participle> ::= @IRREGULAR "været" | "haft" | "blevet" | "gået" | "kommet" | "vidst" | "gjort" | "fået" | "set" | "givet" | "sagt" | "taget" | "fundet" | "stået" | "siddet" | "ligget" | "slået" | "holdt" | "faldet" | "døet" | "født" | "båret" | "spist" | "drukket" | "sovet" | "løbet"

/* Common 'have' verb conjugations */
<have-verb> ::= <have-present> | <have-past> | <have-perfect>

<have-present> ::= @TENSE[present] "har"

<have-past> ::= @TENSE[past] "havde"

<have-perfect> ::= @TENSE[perfect] "har" <opt-whitespace> "haft"

/* Common 'be' verb conjugations */
