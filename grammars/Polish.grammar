/* ===== EXTENDED VERB CONSTRUCTIONS ===== */

/* Aspectual pairs - perfective and imperfective */
<aspectual-pair> ::= <imperfective-verb> | <perfective-verb>

<imperfective-verb> ::= "czytać" | "pisać" | "robić" | "mówić" | "jeść"
                      | "pić" | "brać" | "dawać" | "pytać" | "otwierać"

