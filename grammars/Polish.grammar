                         <opt-whitespace> "ale" <opt-whitespace> ("także" | "również") 
                         <opt-whitespace> <phrase>

/* ===== DIALECTAL VARIATIONS ===== */

@DIALECT[wielkopolski] @DIALECT[malopolski] @DIALECT[mazowiecki] 
@DIALECT[slaski] @DIALECT[kaszubski] @DIALECT[podhalanski] @DIALECT[warszawski]

/* Greater Poland dialect (Wielkopolskie) */
<wielkopolski-pronoun> ::= @DIALECT[wielkopolski] ("my" | "wy" | "wos")

<wielkopolski-verb> ::= @DIALECT[wielkopolski] <verb-stem> ("om" | "amy" | "omy")

/* Lesser Poland dialect (Małopolskie) */
<malopolski-ending> ::= @DIALECT[malopolski] ("ek" | "ka" | "ko" | "uś")

/* Silesian dialect (Śląskie) */
<slaski-pronoun> ::= @DIALECT[slaski] ("ja" | "ty" | "ón" | "óna" | "my" | "wy" | "óni")

<slaski-particle> ::= @DIALECT[slaski] ("też" | "tyż" | "ale" | "fto" | "kaj")

/* Kashubian dialect (Kaszubskie) */
<kaszubski-greeting> ::= @DIALECT[kaszubski] ("witôj" | "dzéń dobri" | "dobri wiecór")

