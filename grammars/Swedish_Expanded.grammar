/* Genitive (possessive) */
<genitive-modifier> ::= <noun-phrase> "s" @CASE[genitive]

/* Compound nouns */
<compound-noun> ::= <noun> <noun>
                 | <noun> <noun> <noun>

/* Proper nouns */
<proper-noun> ::= /[A-ZÅÄÖ][a-zåäö]*/
               | /[A-ZÅÄÖ][a-zåäö]*/ /[A-ZÅÄÖ][a-zåäö]*/  // First and last name

/* Pronouns - comprehensive system */
<pronoun> ::= <personal-pronoun> | <object-pronoun> | <reflexive-pronoun> | <possessive-pronoun>
           | <demonstrative> | <interrogative-pronoun> | <indefinite-pronoun> | <reciprocal-pronoun>

/* Personal pronouns (subject form) */
<personal-pronoun> ::= "jag" @NUMBER[singular] @PERSON[1] @CASE[nominative]
                    | "du" @NUMBER[singular] @PERSON[2] @CASE[nominative]
                    | "han" @NUMBER[singular] @PERSON[3] @GENDER[common] @CASE[nominative]
                    | "hon" @NUMBER[singular] @PERSON[3] @GENDER[common] @CASE[nominative]
                    | "den" @NUMBER[singular] @PERSON[3] @GENDER[common] @CASE[nominative]
                    | "det" @NUMBER[singular] @PERSON[3] @GENDER[neuter] @CASE[nominative]
                    | "vi" @NUMBER[plural] @PERSON[1] @CASE[nominative]
                    | "ni" @NUMBER[plural] @PERSON[2] @CASE[nominative]
                    | "de" @NUMBER[plural] @PERSON[3] @CASE[nominative]
                    | "dom" @NUMBER[plural] @PERSON[3] @CASE[nominative]  // Colloquial

/* Object pronouns */
<object-pronoun> ::= "mig" @NUMBER[singular] @PERSON[1] @CASE[accusative]
                  | "mej" @NUMBER[singular] @PERSON[1] @CASE[accusative]  // Colloquial
                  | "dig" @NUMBER[singular] @PERSON[2] @CASE[accusative]
                  | "dej" @NUMBER[singular] @PERSON[2] @CASE[accusative]  // Colloquial
                  | "honom" @NUMBER[singular] @PERSON[3] @GENDER[common] @CASE[accusative]
                  | "henne" @NUMBER[singular] @PERSON[3] @GENDER[common] @CASE[accusative]
                  | "den" @NUMBER[singular] @PERSON[3] @GENDER[common] @CASE[accusative]
                  | "det" @NUMBER[singular] @PERSON[3] @GENDER[neuter] @CASE[accusative]
                  | "oss" @NUMBER[plural] @PERSON[1] @CASE[accusative]
                  | "er" @NUMBER[plural] @PERSON[2] @CASE[accusative]
                  | "dem" @NUMBER[plural] @PERSON[3] @CASE[accusative]
                  | "dom" @NUMBER[plural] @PERSON[3] @CASE[accusative]  // Colloquial

/* Reflexive pronouns */
<reflexive-pronoun> ::= "mig" @PERSON[1] @NUMBER[singular]
                     | "dig" @PERSON[2] @NUMBER[singular]
                     | "sig" @PERSON[3]
                     | "oss" @PERSON[1] @NUMBER[plural]
                     | "er" @PERSON[2] @NUMBER[plural]
                     | "själv" | "själva"  // Emphatic

/* Possessive pronouns */
<possessive-pronoun> ::= "min" @GENDER[common] @NUMBER[singular] @PERSON[1]
                      | "mitt" @GENDER[neuter] @NUMBER[singular] @PERSON[1]
                      | "mina" @NUMBER[plural] @PERSON[1]
