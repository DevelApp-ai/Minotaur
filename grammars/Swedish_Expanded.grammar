/* Interrogative pronouns */
<interrogative-pronoun> ::= "vem" | "vad" | "vilken" | "vilket" | "vilka" | "vars"

/* Indefinite pronouns */
<indefinite-pronoun> ::= "någon" | "något" | "några"
