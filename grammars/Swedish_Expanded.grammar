                      | "ert" @GENDER[neuter] @NUMBER[singular] @PERSON[2]
                      | "era" @NUMBER[plural] @PERSON[2]
                      | "deras" @PERSON[3]

/* Demonstratives */
<demonstrative> ::= "den här" @GENDER[common] @NUMBER[singular]
                 | "det här" @GENDER[neuter] @NUMBER[singular]
                 | "de här" @NUMBER[plural]
                 | "dessa" @NUMBER[plural]
                 | "den där" @GENDER[common] @NUMBER[singular]
                 | "det där" @GENDER[neuter] @NUMBER[singular]
                 | "de där" @NUMBER[plural]
                 | "denna" @GENDER[common] @NUMBER[singular]
