                  | "blå" @GENDER[common] @NUMBER[singular]
                  | "blått" @GENDER[neuter] @NUMBER[singular]
                  | "blå" @NUMBER[plural]
                  | "blå" @DEFINITENESS[definite]
                  | "grön" @GENDER[common] @NUMBER[singular]
                  | "grönt" @GENDER[neuter] @NUMBER[singular]
                  | "gröna" @NUMBER[plural]
                  | "gröna" @DEFINITENESS[definite]
                  | "svart" @GENDER[common] @NUMBER[singular]
                  | "svart" @GENDER[neuter] @NUMBER[singular]
                  | "svarta" @NUMBER[plural]
                  | "svarta" @DEFINITENESS[definite]
                  | "vit" @GENDER[common] @NUMBER[singular]
                  | "vitt" @GENDER[neuter] @NUMBER[singular]
                  | "vita" @NUMBER[plural]
                  | "vita" @DEFINITENESS[definite]
                  | "snabb" @GENDER[common] @NUMBER[singular]
                  | "snabbt" @GENDER[neuter] @NUMBER[singular]
                  | "snabba" @NUMBER[plural]
                  | "snabba" @DEFINITENESS[definite]
                  | "långsam" @GENDER[common] @NUMBER[singular]
                  | "långsamt" @GENDER[neuter] @NUMBER[singular]
                  | "långsamma" @NUMBER[plural]
                  | "långsamma" @DEFINITENESS[definite]

/* Comparative adjectives */
<adjective-comparative> ::= "större" @DEGREE[comparative]
                         | "mindre" @DEGREE[comparative]
                         | "bättre" @DEGREE[comparative]
                         | "sämre" @DEGREE[comparative]
                         | "äldre" @DEGREE[comparative]
                         | "yngre" @DEGREE[comparative]
                         | "finare" @DEGREE[comparative]
                         | "vackrare" @DEGREE[comparative]
                         | "gladare" @DEGREE[comparative]
                         | "snabbare" @DEGREE[comparative]
                         | "långsammare" @DEGREE[comparative]
                         | "nyare" @DEGREE[comparative]

/* Superlative adjectives */
<adjective-superlative> ::= "störst" @DEGREE[superlative]
                         | "minst" @DEGREE[superlative]
                         | "bäst" @DEGREE[superlative]
                         | "sämst" @DEGREE[superlative]
                         | "äldst" @DEGREE[superlative]
                         | "yngst" @DEGREE[superlative]
                         | "finast" @DEGREE[superlative]
                         | "vackrast" @DEGREE[superlative]
                         | "gladast" @DEGREE[superlative]
                         | "snabbast" @DEGREE[superlative]
                         | "långsammast" @DEGREE[superlative]
                         | "nyast" @DEGREE[superlative]

/* Verb system - comprehensive */
<verb-phrase> ::= <finite-verb>
               | <auxiliary-verb> <supine>
               | <auxiliary-verb> <past-participle>
               | <modal-verb> <infinitive>
               | <verb-passive>

<finite-verb> ::= <verb-present> | <verb-past>

/* Present tense (same for all persons) */
<verb-present> ::= <regular-verb-present> | <irregular-verb-present>

<regular-verb-present> ::= /[a-zåäö]+ar/ @TENSE[present]  // Group 1: -ar
                        | /[a-zåäö]+er/ @TENSE[present]  // Group 2: -er
                        | /[a-zåäö]+r/ @TENSE[present]   // Group 3: -r
                        | /[a-zåäö]+/ @TENSE[present]    // Group 4: no ending

/* Past tense */
<verb-past> ::= <regular-verb-past> | <irregular-verb-past>

<regular-verb-past> ::= /[a-zåäö]+ade/ @TENSE[past]  // Group 1: -ade
                     | /[a-zåäö]+de/ @TENSE[past]   // Group 2: -de
                     | /[a-zåäö]+te/ @TENSE[past]   // Group 3: -te
                     | /[a-zåäö]+dde/ @TENSE[past]  // Group 4: -dde

/* Irregular verbs - comprehensive list (50+ verbs) */
<irregular-verb-present> ::= "är" | "har" | "blir" | "går" | "kommer" | "ser" | "gör" | "säger" | "tar" | "vet"
                          | "bär" | "biter" | "bjuder" | "brister" | "bryter" | "bär" | "dör" | "driver"
                          | "dricker" | "drar" | "eter" | "faller" | "far" | "finner" | "flyger" | "flyter"
                          | "fryser" | "får" | "gråter" | "ger" | "griper" | "gömmer" | "heter" | "hjälper"
                          | "håller" | "kan" | "kliver" | "kryper" | "lider" | "ligger" | "ljuger" | "lyser"
                          | "måste" | "nyser" | "piper" | "rider" | "ringer" | "river" | "ryter" | "ryker"
                          | "sjunger" | "sjunker" | "skiner" | "skiter" | "skrider" | "skriver" | "skriker"
                          | "skjuter" | "ska" | "skär" | "slår" | "sliter" | "sluter" | "smyger" | "snyter"
                          | "spinner" | "spricker" | "springer" | "står" | "sticker" | "stiger" | "stjäl"
                          | "strider" | "stryker" | "sträcker" | "super" | "svarar" | "svider" | "sviker"
                          | "sväljer" | "svär" | "säljer" | "säger" | "söker" | "tiger" | "tjuter" | "träder"
                          | "tvingar" | "väljer" | "vill" | "vinner" | "vrider" | "vågar"

<irregular-verb-past> ::= "var" | "hade" | "blev" | "gick" | "kom" | "såg" | "gjorde" | "sa" | "sade" | "tog" | "visste"
                       | "bar" | "bet" | "bjöd" | "brast" | "bröt" | "dog" | "drev" | "drack" | "drog"
                       | "föll" | "for" | "fann" | "flög" | "flöt" | "frös" | "fick" | "grät" | "gav"
                       | "grep" | "gömde" | "hette" | "hjälpte" | "höll" | "kunde" | "klev" | "kröp"
                       | "led" | "låg" | "ljög" | "lyste" | "nös" | "pep" | "red" | "ringde" | "rev"
                       | "röt" | "rök" | "sjöng" | "sjönk" | "sken" | "sket" | "skred" | "skrev"
                       | "skrek" | "sköt" | "skulle" | "skar" | "slog" | "slet" | "slöt" | "smög"
                       | "snöt" | "spann" | "sprack" | "sprang" | "stod" | "stack" | "steg" | "stal"
                       | "stred" | "strök" | "sträckte" | "söp" | "svarade" | "sved" | "svek"
                       | "svalg" | "svor" | "sålde" | "sa" | "sökte" | "teg" | "tjöt" | "trädde"
                       | "tvang" | "valde" | "ville" | "vann" | "vred" | "vågade"

/* Auxiliary verbs */
<auxiliary-verb> ::= "har" @TENSE[present] | "hade" @TENSE[past]
                  | "är" @TENSE[present] | "var" @TENSE[past]

/* Modal verbs */
<modal-verb> ::= "kan" @MOOD[possibility] | "kunde" @MOOD[possibility] @TENSE[past]
              | "måste" @MOOD[necessity]
              | "vill" @MOOD[volition] | "ville" @MOOD[volition] @TENSE[past]
              | "ska" @MOOD[obligation] | "skulle" @MOOD[obligation] @TENSE[past]
              | "bör" @MOOD[recommendation] | "borde" @MOOD[recommendation] @TENSE[past]
              | "får" @MOOD[permission] | "fick" @MOOD[permission] @TENSE[past]
              | "torde" @MOOD[probability]
              | "må" @MOOD[wish]

/* Supine (Swedish special form for perfect tense) */
<supine> ::= /[a-zåäö]+at/ @FORM[supine]  // Group 1
          | /[a-zåäö]+t/ @FORM[supine]   // Group 2/3
          | /[a-zåäö]+it/ @FORM[supine]  // Irregular
          | "varit" | "haft" | "blivit" | "gått" | "kommit" | "sett" | "gjort" | "sagt" | "tagit" | "vetat"
          | "burit" | "bitit" | "bjudit" | "brustit" | "brutit" | "dött" | "drivit" | "druckit" | "dragit"
          | "ätit" | "fallit" | "farit" | "funnit" | "flugit" | "flutit" | "frusit" | "fått" | "gråtit"
          | "givit" | "gripit" | "gömt" | "hetat" | "hjälpt" | "hållit" | "kunnat" | "klivit" | "krupit"
          | "lidit" | "legat" | "ljugit" | "lyst" | "mått" | "nyst" | "pipit" | "ridit" | "ringt"
          | "rivit" | "rutit" | "rykit" | "sjungit" | "sjunkit" | "skinit" | "skitit" | "skridit"
          | "skrivit" | "skrikit" | "skjutit" | "skolat" | "skurit" | "slagit" | "slitit" | "slutit"
          | "smugit" | "snutit" | "spunnit" | "spruckit" | "sprungit" | "stått" | "stuckit" | "stigit"
          | "stulit" | "stridit" | "strukit" | "sträckt" | "supit" | "svarat" | "svidit" | "svikit"
          | "svalt" | "svurit" | "sålt" | "sagt" | "sökt" | "tigit" | "tjutit" | "trätt" | "tvungit"
          | "valt" | "velat" | "vunnit" | "vridit" | "vågat"

/* Past participle (for passive voice and adjectives) */
<past-participle> ::= /[a-zåäö]+ad/ @FORM[past_participle]  // Group 1: -ad/-at/-ade
                   | /[a-zåäö]+t/ @FORM[past_participle]   // Group 2/3
                   | /[a-zåäö]+dd/ @FORM[past_participle]  // Group 4: -dd/-tt/-dda
                   | /[a-zåäö]+en/ @FORM[past_participle]  // Strong verbs: -en/-et/-na

/* Passive voice (s-passive) */
<verb-passive> ::= /[a-zåäö]+s/ @VOICE[passive]
                | "talas" | "läses" | "görs" | "sägs" | "skrives" | "ses" | "äts"

/* Infinitive */
<infinitive> ::= "att" <infinitive-form>

<infinitive-form> ::= /[a-zåäö]+a/  // Regular infinitive
                   | "vara" | "ha" | "bli" | "gå" | "komma" | "se" | "göra" | "säga" | "ta" | "veta"

<infinitive-phrase> ::= <infinitive> [ <object-np> ]* [ <prepositional-phrase> ]*

<supine-phrase> ::= <supine> [ <object-np> ]* [ <prepositional-phrase> ]*

/* Imperative */
<imperative-verb> ::= /[a-zåäö]+a/ @MOOD[imperative]  // Base form or -a ending
                   | /[a-zåäö]+/ @MOOD[imperative]
                   | "var" | "ha" | "bli" | "gå" | "kom" | "se" | "gör" | "säg" | "ta" | "vet"

/* Present participle */
<present-participle> ::= /[a-zåäö]+ande/ @FORM[present_participle]  // -ande ending
                      | /[a-zåäö]+ende/ @FORM[present_participle]  // -ende ending

/* Adverbs - comprehensive system */
<adverb> ::= <time-adverb> | <place-adverb> | <manner-adverb> | <degree-adverb>
          | <frequency-adverb> | <sentence-adverb>

<time-adverb> ::= "nu" | "då" | "sedan" | "förr" | "alltid" | "aldrig" | "ofta" | "sällan" | "ibland"
               | "idag" | "igår" | "imorgon" | "ikväll" | "inatt" | "ifjol" | "i år" | "förut"
               | "nyligen" | "nyss" | "strax" | "snart" | "genast" | "omedelbart" | "länge"
               | "tidigt" | "sent" | "redan" | "ännu" | "fortfarande" | "hittills" | "förr" | "förra"

<place-adverb> ::= "här" | "där" | "hemma" | "borta" | "ute" | "inne" | "uppe" | "nere"
                | "fram" | "tillbaka" | "framåt" | "bakåt" | "hit" | "dit" | "härifrån" | "därifrån"
                | "hem" | "bort" | "upp" | "ner" | "överallt" | "någonstans" | "ingenstans"

<manner-adverb> ::= "bra" | "väl" | "illa" | "dåligt" | "fort" | "snabbt" | "långsamt" | "lätt" | "svårt"
                 | "rätt" | "fel" | "annorlunda" | "likadant" | "tillsammans" | "ensam" | "själv"
                 | "gärna" | "ogärna" | "helst" | "möjligen" | "verkligen" | "faktiskt"

<degree-adverb> ::= "mycket" | "väldigt" | "ganska" | "rätt" | "för" | "så" | "lite" | "lagom"
                 | "nog" | "alldeles" | "fullständigt" | "helt" | "delvis" | "nästan" | "ungefär"
                 | "precis" | "exakt" | "bara" | "endast" | "blott"

<frequency-adverb> ::= "alltid" | "ofta" | "vanligen" | "vanligtvis" | "brukar" | "ibland" | "stundom"
                    | "sällan" | "aldrig" | "någonsin" | "varje gång" | "varje dag"

<sentence-adverb> ::= "kanske" | "möjligen" | "förmodligen" | "troligen" | "säkert" | "visst"
                   | "nog" | "ju" | "väl" | "faktiskt" | "verkligen" | "egentligen"
                   | "naturligtvis" | "givetvis" | "självklart" | "tyvärr" | "lyckligtvis"
                   | "tack och lov" | "dessvärre" | "också" | "heller" | "bara" | "ändå"

<adverb-phrase> ::= [ <degree-adverb> ]* <adverb>

/* Negation */
<negation> ::= "inte" | "ej" | "icke" | "aldrig" | "ingen" | "inget" | "inga"

/* Prepositions - comprehensive (60+) */
<preposition> ::= <simple-preposition> | <compound-preposition>

<simple-preposition> ::= "i" | "på" | "till" | "från" | "med" | "av" | "för" | "om" | "över"
                      | "under" | "vid" | "hos" | "efter" | "före" | "mot" | "genom" | "runt"
                      | "mellan" | "bland" | "ur" | "utan" | "utom" | "omkring" | "längs"
                      | "kring" | "utefter" | "ovanför" | "nedanför" | "bakom" | "framför"
                      | "bredvid" | "intill" | "invid"

<compound-preposition> ::= "i och med" | "med tanke på" | "på grund av" | "i stället för"
                        | "med hänsyn till" | "i förhållande till" | "i enlighet med"
                        | "i samband med" | "i fråga om" | "med avseende på"
                        | "till följd av" | "till skillnad från" | "i jämförelse med"
                        | "ända till" | "ända fram till" | "ända sedan" | "långt före"
                        | "ända bort till" | "alldeles intill" | "mitt framför"

<prepositional-phrase> ::= <preposition> <noun-phrase>

/* Conjunctions */
<coordinating-conjunction> ::= "och" | "eller" | "men" | "utan" | "för" | "så" | "samt" | "plus" | "liksom"

<subordinating-conjunction> ::= "att" | "om" | "när" | "eftersom" | "medan" | "fastän" | "innan" | "efter"
                             | "sedan" | "trots att" | "även om" | "så att" | "för att"
                             | "såvida" | "så länge" | "tills" | "ända tills" | "så snart som"

/* Question words */
<question-word> ::= "vem" | "vad" | "när" | "var" | "vart" | "varifrån" | "hur" | "varför"
                 | "vilket" | "vilken" | "vilka" | "vars" | "vems"

/* Quantifiers */
<quantifier> ::= "många" | "få" | "flera" | "en del" | "vissa" | "några" | "alla" | "samtliga"
              | "varje" | "var" | "varenda" | "ingen" | "inga" | "allt" | "mycket" | "lite"
              | "mer" | "mindre" | "mest" | "minst" | "tillräckligt" | "andra" | "övriga"

/* Subject and object noun phrases */
<subject-np> ::= <noun-phrase>
<object-np> ::= <noun-phrase>

/* Regional Dialects */

/* SCANIAN DIALECT (Skånska) - Southern Sweden */
<scanian-pronoun> ::= "ja" @DIALECT[scanian]  // jag
                   | "mä" @DIALECT[scanian]  // mig
                   | "dom" @DIALECT[scanian]  // de/dem

<scanian-verb> ::= /[a-zåäö]+ar/ @DIALECT[scanian] @TENSE[present]
                | /[a-zåäö]+a/ @DIALECT[scanian] @TENSE[past]  // Different past forms

/* GOTLANDIC DIALECT (Gutniska) - Gotland island */
<gotlandic-pronoun> ::= "ai" @DIALECT[gotlandic]  // jag (preserves Old Norse)

<gotlandic-diphthong> ::= "ai" @DIALECT[gotlandic]  // Preserved diphthongs
                       | "au" @DIALECT[gotlandic]

/* FINLAND SWEDISH (Finlandssvenska) */
<finland-swedish-pronoun> ::= "jag" @DIALECT[finland_swedish]  // Same as standard but different phonology

<finland-swedish-loanword> ::= "tori" @DIALECT[finland_swedish]  // torget (from Finnish)
                             | "posti" @DIALECT[finland_swedish]  // posten (from Finnish)

/* DALECARLIAN (Dalmål) - Central Sweden */
<dalecarlian-pronoun> ::= "ig" @DIALECT[dalecarlian]  // jag

/* NORRLAND DIALECTS (Norrländska) - Northern Sweden */
<norrland-expression> ::= "ti'stan" @DIALECT[norrland]  // till staden

/* GOTHENBURG DIALECT (Göteborgska) - Western urban */
<gothenburg-expression> ::= "e" @DIALECT[gothenburg]  // är (characteristic reduction)

/* Colloquial Swedish (Talspråk) */
<colloquial-pronoun> ::= "dom" | "nåt" | "nån"
<colloquial-verb> ::= "e" | "va" | "ha"  // är, var, har (reduced forms)
<colloquial-adverb> ::= "nåt" | "nån" | "lite" | "jätte" | "super" | "as"  // Intensifiers

/* Whitespace */
<whitespace> ::= /[ \t\n\r]+/

/* CONTEXT-SENSITIVE VALIDATION RULES */

/* Rule 1: Gender agreement (article-noun) */
VALIDATE gender_agreement_article:
  IF <indefinite-article>.@GENDER != <noun>.@GENDER
  THEN ERROR "Article gender must match noun gender"

/* Rule 2: Number agreement (article-noun) */
VALIDATE number_agreement_article:
  IF <indefinite-article>.@NUMBER != <noun>.@NUMBER
  THEN ERROR "Article number must match noun number"

/* Rule 3: Adjective-noun gender agreement */
VALIDATE adjective_noun_gender:
  IF <adjective>.@GENDER != <noun>.@GENDER
  THEN ERROR "Adjective gender must match noun gender"

/* Rule 4: Adjective-noun number agreement */
VALIDATE adjective_noun_number:
  IF <adjective>.@NUMBER != <noun>.@NUMBER
  THEN ERROR "Adjective number must match noun number"

/* Rule 5: Definite vs indefinite adjective forms */
VALIDATE adjective_definiteness:
  IF <noun>.@DEFINITENESS = definite AND <adjective> NOT ENDS WITH "a"
  THEN ERROR "Definite nouns require adjectives ending in -a"

/* Rule 6: Subject-verb number agreement */
VALIDATE subject_verb_agreement:
  IF <subject-np>.@NUMBER != <finite-verb>.@NUMBER
  THEN WARNING "Subject and verb number should agree (Swedish verbs don't inflect but semantic agreement is expected)"

/* Rule 7: Reflexive pronoun agreement */
VALIDATE reflexive_agreement:
  IF <reflexive-pronoun>.@PERSON != <subject-np>.@PERSON
  THEN ERROR "Reflexive pronoun must agree with subject person"

/* Rule 8: Possessive pronoun gender agreement */
VALIDATE possessive_gender:
  IF <possessive-pronoun>.@GENDER != <noun>.@GENDER
  THEN ERROR "Possessive pronoun gender must match noun gender"

/* Rule 9: V2 word order validation */
VALIDATE v2_word_order:
  IN <declarative-clause>
  IF POSITION(<finite-verb>) != 2
  THEN ERROR "Finite verb must be in second position (V2 word order)"

/* Rule 10: Subordinate clause word order */
VALIDATE subordinate_word_order:
  IN <subordinate-clause>
  IF <finite-verb> PRECEDES <object-np>
  THEN WARNING "In subordinate clauses, verb tends toward final position"

/* Rule 11: Pitch accent assignment */
VALIDATE pitch_accent:
  IF <noun> IS monosyllabic
  THEN <noun>.@PITCH_ACCENT = tone1
  ELSE IF <noun> IS compound
  THEN <noun>.@PITCH_ACCENT = tone1
  ELSE <noun>.@PITCH_ACCENT = tone2

/* Rule 12: Passive voice formation */
VALIDATE passive_formation:
  IF <verb-passive> NOT ENDS WITH "s"
  THEN ERROR "Passive verbs must end in -s"

/* Rule 13: Supine vs participle selection */
VALIDATE supine_usage:
  IF <auxiliary-verb> = "har" OR <auxiliary-verb> = "hade"
  THEN REQUIRE <supine>
  ELSE WARNING "Use supine with 'har/hade', participle with 'är/var'"

/* Rule 14: Modal verb + infinitive structure */
VALIDATE modal_infinitive:
  IF <modal-verb> EXISTS
  THEN REQUIRE <infinitive>
  ELSE ERROR "Modal verbs require infinitive form"

/* Rule 15: Comparative/superlative context */
VALIDATE comparison_context:
  IF <adjective-comparative> EXISTS
  THEN REQUIRE "än" OR implicitcomparison
  ELSE IF <adjective-superlative> EXISTS
  THEN REQUIRE definite_article

/* Rule 16: Relative pronoun-antecedent agreement */
VALIDATE relative_agreement:
  IF <relative-pronoun>.@GENDER != ANTECEDENT.@GENDER
  THEN ERROR "Relative pronoun must match antecedent gender"

/* Rule 17: Demonstrative-noun agreement */
VALIDATE demonstrative_agreement:
  IF <demonstrative>.@GENDER != <noun>.@GENDER
  THEN ERROR "Demonstrative must match noun gender"

/* Rule 18: Nested clause depth warning */
VALIDATE nested_depth:
  IF NESTING_LEVEL > 3
  THEN WARNING "Deeply nested clauses may be difficult to parse"

/* Rule 19: Correlative pair matching */
VALIDATE correlative_pairs:
  IF <correlative-pair-1> = "både"
  THEN REQUIRE <correlative-pair-2> = "och"
  ELSE IF <correlative-pair-1> = "antingen"
  THEN REQUIRE <correlative-pair-2> = "eller"
  ELSE IF <correlative-pair-1> = "varken"
  THEN REQUIRE <correlative-pair-2> = "eller"

/* Rule 20: Dialectal consistency */
VALIDATE dialect_consistency:
  IF @DIALECT EXISTS IN sentence
  THEN ALL forms IN sentence SHOULD match @DIALECT
  ELSE WARNING "Mixing dialects may cause inconsistency"

/* END OF SWEDISH GRAMMAR */
