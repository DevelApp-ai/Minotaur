Grammar: Swedish
TokenSplitter: Space
FormatType: CEBNF
ContextSensitive: true
SymbolTableSharing: hierarchical

/*
 * Swedish Natural Language Grammar - Comprehensive Edition
 * 
 * Comprehensive coverage including:
 * - 6 regional dialects (Scanian, Gotlandic, Finland Swedish, Dalecarlian, Norrland, Göteborgska)
 * - V2 word order with topicalization
 * - Pitch accent system (Tone 1/Tone 2)
 * - Complex nested subordinate clauses (3+ levels)
 * - 50+ irregular verbs with complete paradigms
 * - Supine form (unique to Swedish/Norwegian)
 * - Full pronoun and adjective declension systems
 * - Comparative and superlative forms
 * - Passive voice (s-passive)
 * - Comprehensive prepositional system (60+ prepositions)
 * - 9 types of adverbial clauses
 * - Cleft sentences and correlative constructions
 * - 20 context-sensitive validation rules
 */

Keywords: och, eller, men, utan, för, så, att, om, när, eftersom, medan, fastän, innan, efter, som, vilken, vars, där, vem, vad, hur, varför, jag, du, han, hon, den, det, vi, ni, de, dom, mig, dig, honom, henne, oss, er, dem, min, din, hans, hennes, vår, er, deras, denna, detta, dessa, en, ett, inte, kan, måste, vill, ska, bör, ha, vara, bli, sig, själv, nej, ja, kanske, alltså, dock, också, heller, ju, väl, nog, bara, precis, bra, nå, då

/* Dialect markers for Swedish regional varieties */
@DIALECT[scanian]          /* Skånska - Southern Sweden */
@DIALECT[gotlandic]        /* Gutniska - Gotland island */
@DIALECT[finland_swedish]  /* Finlandssvenska - Finland */
@DIALECT[dalecarlian]      /* Dalmål - Dalarna region */
@DIALECT[norrland]         /* Norrländska - Northern Sweden */
@DIALECT[gothenburg]       /* Göteborgska - Gothenburg */

/* Pitch accent markers */
@PITCH_ACCENT[tone1]  /* Accent 1 - single peak */
@PITCH_ACCENT[tone2]  /* Accent 2 - double peak */

/* Top-level sentence structures */
<sentence> ::= <main-clause>
            | <subordinate-clause>
            | <compound-sentence>
            | <complex-sentence>
            | <nested-complex-sentence>
            | <cleft-sentence>
            | <correlative-sentence>

/* Main clause with V2 word order */
<main-clause> ::= <declarative-clause>
               | <interrogative-clause>
               | <imperative-clause>
               | <exclamative-clause>

/* V2 declarative: Topic + Finite Verb + Subject + Middle Field + Verb Complement */
<declarative-clause> ::= <topic> <finite-verb> <subject-np> [ <middle-field> ]* [ <verb-complement> ]* "."
                      | <subject-np> <finite-verb> [ <middle-field> ]* [ <verb-complement> ]* "."

<topic> ::= <adverb-phrase>
         | <prepositional-phrase>
         | <object-np>
         | <subordinate-clause>

<middle-field> ::= <negation>
                | <sentence-adverb>
                | <object-np>
                | <adverb-phrase>

<verb-complement> ::= <object-np>
                   | <prepositional-phrase>
                   | <adverb-phrase>
                   | <infinitive-phrase>
                   | <supine-phrase>

/* Interrogative clauses */
<interrogative-clause> ::= <question-word> <finite-verb> <subject-np> [ <object-np> ]* [ <prepositional-phrase> ]* "?"
                        | <finite-verb> <subject-np> [ <object-np> ]* [ <prepositional-phrase> ]* "?"
                        | <subject-np> <finite-verb> [ <object-np> ]* "," <tag-question> "?"

<tag-question> ::= "eller hur" | "va" | "inte sant" | "visst"

/* Imperative */
<imperative-clause> ::= <imperative-verb> [ <object-np> ]* [ <prepositional-phrase> ]* "!"

/* Compound sentences */
<compound-sentence> ::= <main-clause> <coordinating-conjunction> <main-clause>
                     | <main-clause> <coordinating-conjunction> <main-clause> <coordinating-conjunction> <main-clause>

/* Complex sentences with subordination */
<complex-sentence> ::= <main-clause> <subordinating-conjunction> <subordinate-clause>
                    | <subordinating-conjunction> <subordinate-clause> "," <main-clause>

/* Nested complex sentences (3-4 levels) */
<nested-complex-sentence> ::= <main-clause> <subordinating-conjunction> <embedded-clause-level-1>
<embedded-clause-level-1> ::= <subject-np> <verb-phrase-subordinate> <subordinating-conjunction> <embedded-clause-level-2>
<embedded-clause-level-2> ::= <subject-np> <verb-phrase-subordinate> [ <subordinating-conjunction> <embedded-clause-level-3> ]
<embedded-clause-level-3> ::= <subject-np> <verb-phrase-subordinate>

/* Subordinate clauses (verb-final tendency) */
<subordinate-clause> ::= <subject-np> [ <middle-field> ]* [ <object-np> ]* <verb-phrase-subordinate>
                      | <relative-clause>
                      | <complement-clause>
                      | <adverbial-clause>

<verb-phrase-subordinate> ::= [ <auxiliary-verb> ]* <main-verb>
                           | [ <modal-verb> ] <infinitive>

/* Relative clauses */
<relative-clause> ::= <relative-pronoun> <subject-np> <verb-phrase>
                   | <relative-pronoun> <verb-phrase> [ <object-np> ]*

<relative-pronoun> ::= "som" | "vilken" @GENDER[common] | "vilket" @GENDER[neuter] | "vilka" @NUMBER[plural]
                    | "vars" @CASE[genitive]
                    | "där" | "dit" | "därifrån"

/* Complement clauses */
<complement-clause> ::= "att" <subject-np> <verb-phrase>
                     | <interrogative-pronoun> <subject-np> <verb-phrase>
                     | "om" <subject-np> <verb-phrase>
                     | "huruvida" <subject-np> <verb-phrase>

/* Adverbial clauses (9 types) */
<adverbial-clause> ::= <temporal-clause>
                    | <causal-clause>
                    | <conditional-clause>
                    | <concessive-clause>
                    | <purpose-clause>
                    | <result-clause>
                    | <comparative-clause>
                    | <manner-clause>
                    | <place-clause>

<temporal-clause> ::= <temporal-conjunction> <subject-np> <verb-phrase>
<temporal-conjunction> ::= "när" | "medan" | "innan" | "efter" | "sedan" | "så snart som" | "tills" | "ända tills"

<causal-clause> ::= <causal-conjunction> <subject-np> <verb-phrase>
<causal-conjunction> ::= "eftersom" | "därför att" | "då" | "emedan" | "för att"

<conditional-clause> ::= <conditional-conjunction> <subject-np> <verb-phrase>
<conditional-conjunction> ::= "om" | "ifall" | "såvida" | "förutsatt att" | "i fall att"

<concessive-clause> ::= <concessive-conjunction> <subject-np> <verb-phrase>
<concessive-conjunction> ::= "fast" | "fastän" | "även om" | "trots att" | "oaktat"

<purpose-clause> ::= <purpose-conjunction> <subject-np> <verb-phrase>
<purpose-conjunction> ::= "för att" | "så att" | "med avsikt att"

<result-clause> ::= <result-conjunction> <subject-np> <verb-phrase>
<result-conjunction> ::= "så att" | "såpass att" | "till den grad att"

<comparative-clause> ::= <comparative-conjunction> <subject-np> <verb-phrase>
<comparative-conjunction> ::= "som" | "än" | "likesom" | "som om"

<manner-clause> ::= <manner-conjunction> <subject-np> <verb-phrase>
<manner-conjunction> ::= "som" | "som om" | "utan att"

<place-clause> ::= <place-conjunction> <subject-np> <verb-phrase>
<place-conjunction> ::= "där" | "dit" | "därifrån" | "varifrån" | "vart"

/* Cleft sentences */
<cleft-sentence> ::= "Det" "är" <focus-np> "som" <subject-np> <verb-phrase> "."
                  | <focus-wh-word> <subject-np> <verb-phrase> "," "är" <noun-phrase> "."

<focus-np> ::= <noun-phrase> | <prepositional-phrase>
<focus-wh-word> ::= "Vad" | "Vem" | "Var" | "När" | "Hur"

/* Correlative constructions */
<correlative-sentence> ::= <correlative-pair-1> <clause-1> <correlative-pair-2> <clause-2>

<correlative-pair-1> ::= "både" | "antingen" | "varken" | "ju" | "inte bara" | "dels"
<correlative-pair-2> ::= "och" | "eller" | "eller" | "desto" | "utan också" | "dels"

<clause-1> ::= <subject-np> <verb-phrase>
<clause-2> ::= <subject-np> <verb-phrase>

/* Noun phrases */
<noun-phrase> ::= [ <determiner> ] [ <adjective-phrase> ]* <noun-head> [ <genitive-modifier> ]* [ <relative-clause> ]*
               | <pronoun>
               | <proper-noun>
               | <compound-noun>

<noun-head> ::= <noun-common> | <noun-neuter>

/* Two-gender system */
<determiner> ::= <indefinite-article> | <definite-article> | <demonstrative> | <possessive-pronoun> | <quantifier>

<indefinite-article> ::= "en" @GENDER[common] @NUMBER[singular] @DEFINITENESS[indefinite]
                      | "ett" @GENDER[neuter] @NUMBER[singular] @DEFINITENESS[indefinite]

<definite-article> ::= "den" @GENDER[common] @NUMBER[singular] @DEFINITENESS[definite]
                    | "det" @GENDER[neuter] @NUMBER[singular] @DEFINITENESS[definite]
                    | "de" @NUMBER[plural] @DEFINITENESS[definite]

