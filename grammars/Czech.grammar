/* ===== ADDITIONAL CONTEXT RULES ===== */

@CONTEXT_RULE[pronoun-case-agreement]:
    IF <prepositional-phrase> CONTAINS <pronoun>
    THEN <pronoun>.@CASE == <preposition>.@REQUIRED_CASE
    ELSE WARNING "Pronoun case must agree with preposition requirement"

@CONTEXT_RULE[genitive-of-quantity]:
    IF <noun-phrase> STARTS_WITH <cardinal-numeral> AND <cardinal-numeral> > "čtyři"
    THEN <noun>.@CASE == genitive
    ELSE VALID

@CONTEXT_RULE[temporal-case-requirement]:
    IF <temporal-expression> CONTAINS <preposition>
    THEN <noun>.@CASE == <preposition>.@REQUIRED_CASE
    ELSE VALID

@CONTEXT_RULE[comparative-case-government]:
    IF <comparative-construction> USES "než"
    THEN <noun-phrase>.@CASE == nominative
    ELSE IF <comparative-construction> USES "od"
    THEN <noun-phrase>.@CASE == genitive
    ELSE VALID

@CONTEXT_RULE[quantifier-agreement]:
    IF <noun-phrase> CONTAINS <quantifier>
    THEN <quantifier> AGREES_WITH <noun> IN @NUMBER
    ELSE VALID

